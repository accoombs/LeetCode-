def minMoves(self, target, maxDoubles):
        steps = 0
        while(target > 1):
            if(maxDoubles > 0 and target%2 == 0):
                target/=2
                steps+=1
                maxDoubles-=1
            else:
                target-=1
                steps+=1
            if maxDoubles == 0:
                steps+=(target-1)
                target = 1
                
        return steps
