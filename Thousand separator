def thousandSeparator(self, n):
        if(n < 1000):
            return str(n)
        toReturn = (str(n))[0]
        number = str(n)
        for i in range(len(number)-2, -1, -1):
            if((i+1)%3 == 0):
                toReturn+='.'
            toReturn += number[len(number)-1-i]
        return toReturn
