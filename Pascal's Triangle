def generate(self, numRows):
        arr = []
        
        for i in range(1, numRows+1):
            row = [1]*(i)
            arr.append(row)
        
        for i in range(1, len(arr)-1):
            for c in range(1,len(arr[i])):
                arr[i+1][c] = arr[i][c-1]+arr[i][c]
        
        return arr
