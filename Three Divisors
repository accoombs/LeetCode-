def isThree(self, n):
        if(n%2 == 0 and n!=4):
            return False
        if n%2 == 1 and n<9:
            return False
        
        sqrt = math.sqrt(n)
        if(sqrt%1==0):
            for i in range(int(sqrt)-1, 1, -1):
                if(sqrt%i==0):
                    return False
        else: return False
        return True
